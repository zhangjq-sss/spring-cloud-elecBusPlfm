<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.payment.mapper.PaymentLogMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.domain.payment.PaymentLog" id="paymenlogMap">
        <result property="id" column="id"/>
        <result property="payRecordId" column="pay_record_id"/>
        <result property="orderNo" column="order_no"/>
        <result property="thirdNo" column="third_no"/>
        <result property="resultMsg" column="result_msg"/>
        <result property="resultCode" column="result_code"/>
        <result property="payIp" column="pay_ip"/>
        <result property="operType" column="oper_type"/>
        <result property="rspType" column="rsp_type"/>
        <result property="crtTime" column="crt_time"/>
        <result property="updTime" column="upd_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

</mapper>