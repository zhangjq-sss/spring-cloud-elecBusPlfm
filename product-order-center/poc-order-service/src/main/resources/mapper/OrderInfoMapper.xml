<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.order.mapper.OrderInfoMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.domain.order.OrderInfo" id="orderInfoMap">
        <result property="id" column="id"/>
        <result property="orderNo" column="order_no"/>
        <result property="itemCount" column="item_count"/>
        <result property="custId" column="cust_id"/>
        <result property="shopId" column="shop_id"/>
        <result property="crtTime" column="crt_time"/>
        <result property="payType" column="pay_type"/>
        <result property="outTradeNo" column="out_trade_no"/>
        <result property="payTime" column="pay_time"/>
        <result property="mailMethod" column="mail_method"/>
        <result property="expectMarlTime" column="expect_marl_time"/>
        <result property="totalPrice" column="total_price"/>
        <result property="mailFee" column="mail_fee"/>
        <result property="totalDiscountFee" column="total_discount_fee"/>
        <result property="actualPayAmount" column="actual_pay_amount"/>
        <result property="version" column="version"/>
        <result property="status" column="status"/>
        <result property="updTime" column="upd_time"/>
    </resultMap>

</mapper>